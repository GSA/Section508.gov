{
  "name": "federalist-uswds-jekyll",
  "version": "1.3.0",
  "scripts": {
    "build": "bundle exec jekyll build",
    "clean": "bundle exec jekyll clean",
    "reset": "npx rimraf .git",
    "start": "bundle exec jekyll serve",
    "test": "bundle exec htmlproofer _site --http-status-ignore '0,403'; pa11y-ci '_site/**/*.html'",
    "ng": "ng",
    "start:ang": "ng serve --configuration development",
    "build:ang": "ng build --configuration dev",
    "watch": "ng build --watch --configuration development",
    "test:ang": "ng test",
    "federalist": " bash ./angular-build.sh; "
  },
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/forms": "^19.2.14",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/router": "^19.2.14",
    "@fortawesome/fontawesome-free": "~6.2.0",
    "@uswds/uswds": "^3.7.1",
    "clipboard-copy": "^4.0.1",
    "docx": "^8.0.2",
    "jszip": "^3.10.1",
    "nanoid": "^5.0.9",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "~19.2.15",
    "@angular/compiler-cli": "^19.2.14",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "rimraf": "^3.0.2",
    "typescript": "~5.8.3"
  }
}
