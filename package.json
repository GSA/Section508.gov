{
  "name": "federalist-uswds-jekyll",
  "version": "1.3.0",
  "scripts": {
    "build": "bundle exec jekyll build",
    "clean": "bundle exec jekyll clean",
    "reset": "npx rimraf .git",
    "start": "bundle exec jekyll serve",
    "test": "bundle exec htmlproofer _site --http-status-ignore '0,403'; pa11y-ci '_site/**/*.html'",
    "ng": "ng",
    "start:ang": "ng serve --configuration development",
    "build:ang": "ng build --configuration dev",
    "watch": "ng build --watch --configuration development",
    "test:ang": "ng test",
    "federalist": " bash ./angular-build.sh; "
  },
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@fortawesome/fontawesome-free": "~6.2.0",
    "uswds": "~2.13.2",
    "@uswds/uswds": "~3.2.0",
    "docx": "^8.0.2",
    "ngx-clipboard": "^15.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular/cli": "~15.2.8",
    "@angular/compiler-cli": "^15.2.9",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "pa11y-ci": "^3.0.1",
    "typescript": "~4.9.3",
    "rimraf": "^3.0.2"
  }
}